{
    "name": "zippy",
    "version": "0.1.0",
    "private": true,
    "author": "Roman Ranniew",
    "description": "Modern archives manager",
    "scripts": {
        "dev": "npm run electron:serve",
        "electron:build": "cross-env NODE_ENV=production vue-cli-service electron:build --windows",
        "electron:serve": "vue-cli-service electron:serve",
        "postinstall": "electron-builder install-app-deps",
        "postuninstall": "electron-builder install-app-deps"
    },
    "main": "background.js",
    "dependencies": {
        "7zip": "^0.0.6",
        "core-js": "^3.6.5",
        "cross-env": "^7.0.3",
        "date-fns": "^2.22.1",
        "electron-store": "^8.0.0",
        "extract-zip": "^1.7.0",
        "fs-extra": "^7.0.1",
        "glob": "^7.1.7",
        "materialize-css": "^1.0.0",
        "pretty-bytes": "^5.6.0",
        "regedit": "^3.0.3",
        "vue": "^3.0.0",
        "vue-router": "^4.0.8",
        "vuex": "^4.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.0",
        "babel-eslint": "^10.1.0",
        "electron": "^12.0.9",
        "electron-devtools-installer": "^3.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^7.0.0",
        "node-sass": "^6.0.0",
        "sass": "^1.34.0",
        "sass-loader": "^10.1.1",
        "vue-cli-plugin-electron-builder": "~2.0.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/vue3-essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {}
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
